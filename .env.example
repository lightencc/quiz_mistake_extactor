GOOGLE_API_KEY=your_google_api_key_here
GEMINI_MODEL=gemini-3-flash-preview
# 可选：使用代理或兼容网关时可配置
GEMINI_BASE_URL=
GEMINI_API_VERSION=
GEMINI_REQUEST_TIMEOUT_SECONDS=90
GEMINI_OCR_MODEL=gemini-3-flash-preview

# 仅兼容旧模型名变量；google-genai 不会读取 OPENAI_API_KEY / BASE_URL
OPENAI_API_KEY=
BASE_URL=
MODEL=
MISTAKE_DETAIL=high

# GitHub 图床（导出前上传裁剪图片，Markdown 使用 raw 链接）
GITHUB_TOKEN=ghp_xxx
GITHUB_REPO=lightencc/quiz_content
GITHUB_BRANCH=main
GITHUB_IMAGE_DIR=images
GITHUB_RAW_BASE=https://raw.githubusercontent.com
# 上传前压缩（只上传压缩图，不上传原裁剪图）
UPLOAD_COMPRESS_MAX_SIDE=1800
UPLOAD_COMPRESS_JPEG_QUALITY=82

# Notion（导出完成后自动上传）
NOTION_API_KEY=secret_xxx
NOTION_DATABASE_ID=xxxxxxxxxxxxxxxxxxxxxxxxxxxxxxxx
# 可选：新版 Notion 建议直接填数据源 ID（优先级高于 NOTION_DATABASE_ID）
NOTION_DATA_SOURCE_ID=
# 可选：标题属性名，默认自动识别 title 属性
NOTION_TITLE_PROPERTY=
# 可选：ID 字段名，默认 ID；若找不到会自动回退 unique_id 字段
NOTION_ID_PROPERTY=ID
# 可选：标题前缀；最终标题格式 YYYY-MMDD-<prefix>-<ID值>
NOTION_TITLE_PREFIX=
